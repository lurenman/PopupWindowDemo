<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="typeface_style_13_666666">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">#666666</item>
    </style>
    <style name="typeface_style_13_e55995">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">#e55995</item>
    </style>
    <style name="typeface_style_14_333333">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#333333</item>
    </style>
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>
    <style name="AnimTop" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

</resources>
